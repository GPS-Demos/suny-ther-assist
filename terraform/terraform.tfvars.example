# Example Terraform variables file
# Copy this to terraform.tfvars and update with your actual values

# Google Cloud Project ID (required)
project_id = "your-gcp-project-id"

# Google Cloud region for deployments
region = "us-central1"

# Authentication configuration
auth_allowed_domains = "google.com,yourdomain.com"
auth_allowed_emails = "user1@example.com,user2@example.com"
